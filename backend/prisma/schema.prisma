

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL") 
}


model User{
  id String @id @default(uuid())
  email String @unique
  password String
  name String
  gender String?
  photo String?
  isAdmin Boolean @default(false)

  createdAt DateTime @default(now())
}